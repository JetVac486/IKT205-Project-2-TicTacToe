<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.uia.ikt205.tictactoe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="no.uia.ikt205.tictactoe.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThGame"
        android:usesCleartextTraffic="true">
        <activity android:name="no.uia.ikt205.tictactoe.GameActivity"></activity>
        <activity android:name="no.uia.ikt205.tictactoe.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>